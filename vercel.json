{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://job-finder-l5ed.onrender.com/api/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache"
                }
            ]
        }
    ]
}
